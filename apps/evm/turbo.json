{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "env": [
        "ACADEMY_URL",
        "ALCHEMY_ID",
        "BLOG_URL",
        "DRPC_ID",
        "EDGE_CONFIG",
        "FURO_URL",
        "TOKEN_LIST_PR_WEBHOOK_URL",
        "OCTOKIT_KEY",
        "SWAP_API_V0_BASE_URL",
        "POOLS_API_V0_BASE_URL",
        "TOKENS_API_V0_BASE_URL",
        "NEXT_TELEMETRY_DISABLED"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["build"],
      "env": [
        "ALCHEMY_ID",
        "ANVIL_FORK_URL",
        "ANVIL_BLOCK_NUMBER",
        "ANVIL_PORT",
        "APP_ENV",
        "CHAIN_ID",
        "DRPC_ID",
        "EDGE_CONFIG",
        "PORT"
      ]
    },
    "check": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
